<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nju.ar.mapper.dataGetter">
	
  
 	<select id="getAllFinalMovie"  resultType="movie">
 		select * from finalMovie
 	</select>
 	
 	<select id="getAllYupiaoerMovie"  resultType="movie">
 		select * from yupiaoerMovie
 	</select>
 	
    <select id="getAllTaopiaopiaoMovie"  resultType="movie">
 		select * from taopiaopiaoMovie
 	</select>
 	
    <select id="getAllBaiduMovie"  resultType="movie">
 		select * from baiduMovie
 	</select> 

 	<select id="getAllYupiaoerCinema"  resultType="cinema">
 		select * from yupiaoerCinema
 	</select>
 	
    <select id="getAllTaopiaopiaoCinema"  resultType="cinema">
 		select * from taopiaopiaoCinema
 	</select>
 	
 	<select id="getAllBaiduCinema"  resultType="cinema">
 		select * from baiduCinema
 	</select> 
 	
 	<select id="getAllYupiaoerSession"  resultType="session">
 		select * from yupiaoerSession
 	</select>
 	
    <select id="getAllTaopiaopiaoSession"  resultType="session">
 		select * from taopiaopiaoSession
 	</select>
 	
 	<select id="getAllBaiduSession"  resultType="session">
 		select * from baiduSession
 	</select> 
 	
 	<select id="getYupiaoerMovieByMid"  resultType="movie">
 		select * from yupiaoerMovie where movieId = #{mid}
 	</select>
 	
 	<select id="getTaopiaopiaoMovieByMid"  resultType="movie">
 		select * from taopiaopiaoMovie where movieId = #{mid}
 	</select>
 	
 	<select id="getBaiduMovieByMid"  resultType="movie">
 		select * from baiduMovie where movieId = #{mid}
 	</select>
 	
 	 	<select id="getYupiaoerCinemaByCid"  resultType="cinema">
 		select * from yupiaoerCinema where cinemaId = #{cid}
 	</select>
 	
 	<select id="getTaopiaopiaoCinemaByCid"  resultType="cinema">
 		select * from taopiaopiaoCinema where cinemaId = #{cid}
 	</select>
 	
 	<select id="getBaiduCinemaByCid"  resultType="cinema">
 		select * from baiduCinema where cinemaId = #{cid}
 	</select>
 	
 	<select id="getFinalMovieByName"  resultType="movie">
 		select * from finalMovie where name = #{0}
 	</select>
 	
 	<select id="getFinalCinemaByName"  resultType="cinema">
 		select * from finalCinema where name = #{0}
 	</select>
</mapper>
